<odoo>

    <data>
        <!-- Define the list view for exorcismo.puntuacion -->
        <record id="view_game_list" model="ir.ui.view">
            <field name="name">game.list</field>
            <field name="model">exorcismo.puntuacion</field>
            <field name="arch" type="xml">
                <tree string="Game List">
                    <field name="date"/>
                    <field name="name"/>
                    <field name="score"/>
                </tree>
            </field>
        </record>
    </data>




</odoo>
