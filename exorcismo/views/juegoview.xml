<odoo>
    <data>


        <record id="view_game_list" model="ir.ui.view">
            <field name="name">game.list</field>
            <field name="model">exorcismo.puntuacion</field>
            <field name="type">tree</field>
            <field name="arch" type="xml">
                <tree>
                    <header>
                        <button name="jugar" string="Jugar" type="object" class="btn-primary"/>
                    </header>
                    <field name="date"/>
                    <field name="name"/>
                    <field name="score"/>
                </tree>
            </field>
        </record>


        <record id="your_modal_view_id" model="ir.ui.view">
        <field name="name">your.modal.view</field>
        <field name="model">exorcismo.puntuacion</field>
        <field name="arch" type="xml">
            <form string="Exorcismo" create="false" edit="false" modal="true">
                <group>
                    <iframe src="https://htmlpreview.github.io/?https://github.com/DarioMachado/odooclase/blob/main/index.html" width="100%" height="550"/>
                </group>
            </form>
        </field>
        </record>


    </data>





</odoo>
